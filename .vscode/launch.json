    "version": "0.2.0",
    "configurations": [
        {
            "name": "Attach",
            "port": 9229,
            "request": "attach",
            "skipFiles": [
                "<node_internals>/**",
                "${workspaceFolder}/node_modules/**"
            ],
            "type": "node"
        },
        {
            "command": "npm start",
            "name": "start Prod",
            "request": "launch",
            "type": "node-terminal",
            "skipFiles": [
                "<node_internals>/**",
                "${workspaceFolder}/node_modules/**"
            ],
            "cwd": "${workspaceRoot}"
        },
        {
            "command": "npm run start:dev",
            "name": "start Dev",
            "request": "launch",
            "type": "node-terminal",
            "skipFiles": [
                "<node_internals>/**",
                "${workspaceFolder}/node_modules/**"
            ],
            "cwd": "${workspaceRoot}"
        },
        {
            "command": "npm t",
            "name": "test",
            "request": "launch",
            "type": "node-terminal",
            "skipFiles": [
                "<node_internals>/**",
                "${workspaceFolder}/node_modules/**"
            ],
            "cwd": "${workspaceRoot}"
        },
        {
            "command": "npm run test:dev",
            "name": "dev",
            "request": "launch",
            "type": "node-terminal",
            "skipFiles": [
                "<node_internals>/**",
                "${workspaceFolder}/node_modules/**"
            ],
            "cwd": "${workspaceRoot}"
        },
        {
            "command": "npm run test:coverage",
            "name": "coverage",
            "request": "launch",
            "type": "node-terminal",
            "skipFiles": [
                "<node_internals>/**",
                "${workspaceFolder}/node_modules/**"
            ],
        },
        {
            "command": "npm run build",
            "name": "Build",
            "request": "launch",
            "type": "node-terminal",
            "skipFiles": [
                "<node_internals>/**",
                "${workspaceFolder}/node_modules/**"
            ],
            
        },
        {
            "command": "npm run clear",
            "name": "Clear",
            "request": "launch",
            "type": "node-terminal",
            "skipFiles": [
                "<node_internals>/**",
                "${workspaceFolder}/node_modules/**"
            ],
        },
    ]
}